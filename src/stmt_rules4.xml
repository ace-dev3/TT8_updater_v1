<?xml version="1.0" encoding="UTF-8"?>

<stmt_rules>
	<stmt_rule>
		<signature>WbdCache.findWidgetByFullName\(([\w]+?),[\s]*new[\s]+WidgetId\(([\w]+?)\)\)</signature>
		<add_param_in>replace_as_is</add_param_in>
		<param_name>generator.getProject().findWidget($1, new WidgetId($2))</param_name>
	</stmt_rule>
</stmt_rules>

